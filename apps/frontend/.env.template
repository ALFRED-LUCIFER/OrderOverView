# Frontend Environment Variables Template
# Copy this to .env in apps/frontend/ and update the values

# Voice Interface
VITE_ENABLE_VOICE=true

# ElevenLabs TTS Service (Replace with your actual API key)
VITE_ELEVENLABS_API_KEY=your_elevenlabs_api_key_here

# Development URLs (for local development)
VITE_API_URL="http://localhost:3001"
VITE_WEBSOCKET_URL="ws://localhost:3001"

# Production URLs (update these with your actual Vercel URLs)
# VITE_API_URL="https://your-backend-url.vercel.app"
# VITE_WEBSOCKET_URL="wss://your-backend-url.vercel.app"
