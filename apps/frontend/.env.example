# Frontend Environment Variables
# Copy this file to .env and fill in your actual values

# API Configuration
VITE_API_URL="http://localhost:3001"
VITE_WEBSOCKET_URL="ws://localhost:3001"

# Voice Features - Phase 1 & 2 Enhancements
VITE_ENABLE_VOICE="true"
VITE_ELEVENLABS_API_KEY="your_elevenlabs_api_key_here"

# Voice Activity Detection Configuration
VITE_VAD_THRESHOLD=0.3
VITE_VAD_SAMPLE_RATE=16000

# TTS (Text-to-Speech) Configuration
VITE_PREFERRED_TTS_PROVIDER="elevenlabs"  # browser, elevenlabs, openai
VITE_TTS_VOICE_ID="EXAVITQu4vr4xnSDxMaL"  # ElevenLabs voice ID for LISA

# Application Settings
VITE_APP_NAME="Glass Order Management System"
VITE_APP_VERSION="1.0.0"
